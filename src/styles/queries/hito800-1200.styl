@media (min-width: 801px) and (max-width: 1220px)
  header
    & caja1
      position absolute
      top 0
      left 5.4vw
      width 100%
      height auto
      & #textologo
        padding-left 0
        font-size 18pt
      & #idimg
        width 60px
        height 60px
        text-align right
        display inline-block
  $img = 11vw
  section
    & #sideMenu
      font-size 10pt
      width 20%
    & article
      padding 25px 0 0 5px
      width 79%
      & .celda
        display inline-block
        width $img
        margin 0
        padding 0 0 10px 0
        & .desc,
        & .nom
          width $img
        & .img
          width $img
          & img
            width $img
            height $img
      & #empresa
        padding 20px 10px
        height 85%
        & #derecho
          width 47%
          height 80.5%
          & #acerca,
          & #valores
            margin 0
            padding 0
          & #acerca h1,
          & #valores h1
            font-size 14pt
          & #acerca p,
          & #valores p
            font-size 10pt
  footer
    font-size 12pt
    padding-top 5px
    address
      margin-top 5px
